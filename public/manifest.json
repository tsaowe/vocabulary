{
  "name": "曹巍的生词本",
  "description": "添加日常不熟悉的单词",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["./dexie.js", "./background.js"]
  },
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "webRequest"
  ],
  "icons": {
    "16": "icon/library-16x16.png",
    "32": "icon/library-32x32.png",
    "48": "icon/library-48x48.png",
    "128": "icon/library-128x128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
