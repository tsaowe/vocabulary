{
  "name": "Max's Vocabulary Book",
  "description": "Add your own words to the book shelf",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["./mark.min.js", "./dexie.js", "./background.js"]
  },
  "web_accessible_resources": [
    "mark.min.js",
    "dexie.js",
    "page.js",
    "inject.js"
  ],
  "permissions": [
    "contextMenus",
    "storage",
    "tabs",
    "webRequest"
  ],
  "icons": {
    "16": "icon/library-16x16.png",
    "32": "icon/library-32x32.png",
    "48": "icon/library-48x48.png",
    "128": "icon/library-128x128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
